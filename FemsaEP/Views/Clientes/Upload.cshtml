@{
    ViewBag.Title = "Upload";
    Layout = "~/Views/Shared/_LayoutAjax.cshtml";
}


@using (Html.BeginForm(null, null, FormMethod.Post, new { id = "formuploadcliente", enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()

        @Html.ValidationSummary(true)
    
<script>
        $(function () {
        // your code goes here
        $("#btuploadcliente").click(function (data) {
            $("#formuploadcliente").submit();
        });
    });
</script>

        <div class="form-group">
            <label class="col-md-4 control-label">Importar Excel(xls)</label>
            <div class="col-md-10">
                <div class="file">
                    @Html.TextBox("upload", "", new { type = "file", @class = "form-control" })
                </div>
            </div>
        </div>
}

